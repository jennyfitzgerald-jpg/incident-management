# Copy this into Streamlit Cloud → App → Settings → Secrets (paste as TOML).
# Replace placeholders. After first deploy, set OAUTH_REDIRECT_URI to your app URL.

ENV = "production"
OAUTH_PROVIDER = "google"
OAUTH_REDIRECT_URI = "https://YOUR-APP-NAME.streamlit.app/"
SESSION_SECRET_KEY = "generate-a-long-random-string-at-least-32-chars"
GOOGLE_CLIENT_ID = "your-google-client-id.apps.googleusercontent.com"
GOOGLE_CLIENT_SECRET = "your-google-client-secret"
SESSION_TIMEOUT_MINUTES = "60"
ADMIN_USERS = "admin@yourdomain.com"
